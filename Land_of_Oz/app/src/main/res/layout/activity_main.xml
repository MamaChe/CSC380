<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent"
    tools:context=".MainActivityFragment">
    <RelativeLayout
        android:layout_width="fill_parent"
        android:id="@+id/menuStuff"
        android:layout_height="wrap_content">
        <AutoCompleteTextView xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/autocomplete_startingPoint"
            android:layout_width="fill_parent"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_toLeftOf="@+id/buttonUp"
            android:hint="Start"
            android:layout_height="wrap_content" />
        <AutoCompleteTextView xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/autocomplete_destination"
            android:layout_width="fill_parent"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/autocomplete_startingPoint"
            android:layout_toLeftOf="@+id/buttonUp"
            android:hint="Destination"
            android:layout_height="wrap_content" />
        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="▲"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:id="@+id/buttonUp"
            android:onClick="up"
            />
        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="■"
            android:layout_alignParentRight="true"
            android:layout_below="@id/buttonUp"
            android:id="@+id/buttonR"
            android:onClick="rotate"
            />
        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_below="@id/buttonR"
            android:text="▼"
            android:id="@+id/buttonDown"
            android:onClick="down"
            />
        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="go"
            android:layout_alignBaseline="@id/buttonDown"
            android:layout_toLeftOf="@id/buttonDown"
            android:id="@+id/testButton"
            android:onClick="test"
            />
        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/abc_ic_menu_moreoverflow_mtrl_alpha"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/buttonR"
            android:contentDescription="settings"
            android:id="@+id/settings"
            android:onClick="showPopup" />
        <TextView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_toLeftOf="@id/buttonDown"
            android:textStyle="bold"
            android:text="4U"
            android:id="@+id/floorTextView"
            android:textSize="@dimen/abc_text_size_headline_material"
            android:textColor="#000000"
            android:layout_alignBaseline="@id/buttonDown"
            android:layout_toRightOf="@id/settings"/>

    </RelativeLayout>
    <com.group4.land_of_oz.MapViewGroup
        android:id="@+id/MapViewGroup"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="@color/background_floating_material_light"
        >
        <com.group4.land_of_oz.MapView
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:id="@+id/MapView0"
            android:src="@drawable/floorg"
            android:scaleType="centerCrop"
            />
        <com.group4.land_of_oz.MapView
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:id="@+id/MapView1"
            android:src="@drawable/floor1"
            android:scaleType="centerCrop"
            />
        <com.group4.land_of_oz.MapView
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:id="@+id/MapView2"
            android:src="@drawable/floor2l"
            android:scaleType="centerCrop"
            />
        <com.group4.land_of_oz.MapView
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:id="@+id/MapView3"
            android:src="@drawable/floor2u"
            android:scaleType="centerCrop"
            />
        <com.group4.land_of_oz.MapView
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:id="@+id/MapView4"
            android:src="@drawable/floor3l"
            android:scaleType="centerCrop"
            />
        <com.group4.land_of_oz.MapView
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:id="@+id/MapView5"
            android:src="@drawable/floor3u"
            android:scaleType="centerCrop"
            />
        <com.group4.land_of_oz.MapView
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:id="@+id/MapView6"
            android:src="@drawable/floor4l"
            android:scaleType="centerCrop"
            />
        <com.group4.land_of_oz.MapView
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:id="@+id/MapView7"
            android:src="@drawable/floor4u"
            android:scaleType="centerCrop"
            />
    </com.group4.land_of_oz.MapViewGroup>
</LinearLayout>