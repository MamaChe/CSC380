<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent"
    tools:context=".MainActivityFragment">
    <AutoCompleteTextView xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/autocomplete_startingPoint"
        android:layout_width="fill_parent"
        android:hint="Start"
        android:layout_height="wrap_content" />
    <AutoCompleteTextView xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/autocomplete_destination"
        android:layout_width="fill_parent"
        android:hint="Destination"
        android:layout_height="wrap_content" />
    <FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent">
        <com.group4.land_of_oz.MapViewGroup
            android:id="@+id/MapViewGroup"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            >
            <com.group4.land_of_oz.MapView
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:id="@+id/MapView0"
                android:src="@drawable/floorg"
                android:scaleType="centerCrop"
                />
            <com.group4.land_of_oz.MapView
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:id="@+id/MapView1"
                android:src="@drawable/floor1"
                android:scaleType="centerCrop"
                />
            <com.group4.land_of_oz.MapView
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:id="@+id/MapView2"
                android:src="@drawable/floor2l"
                android:scaleType="centerCrop"
                />
            <com.group4.land_of_oz.MapView
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:id="@+id/MapView3"
                android:src="@drawable/floor2u"
                android:scaleType="centerCrop"
                />
            <com.group4.land_of_oz.MapView
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:id="@+id/MapView4"
                android:src="@drawable/floor3l"
                android:scaleType="centerCrop"
                />
            <com.group4.land_of_oz.MapView
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:id="@+id/MapView5"
                android:src="@drawable/floor3u"
                android:scaleType="centerCrop"
                />
            <com.group4.land_of_oz.MapView
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:id="@+id/MapView6"
                android:src="@drawable/floor4l"
                android:scaleType="centerCrop"
                />
            <com.group4.land_of_oz.MapView
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:id="@+id/MapView7"
                android:src="@drawable/floor4u"
                android:scaleType="centerCrop"
                />
        </com.group4.land_of_oz.MapViewGroup>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_gravity="right">
        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="▲"
            android:id="@+id/buttonUp"
            android:onClick="up"
            />
        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="■"
            android:id="@+id/buttonR"
            android:onClick="rotate"
            />
        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="▼"
            android:id="@+id/buttonDown"
            android:onClick="down"
            />
        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Test"
            android:id="@+id/testButton"
            android:onClick="test"
            />
    </LinearLayout>
    </FrameLayout>
</LinearLayout>